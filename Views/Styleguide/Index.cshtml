    <form class="form-horizontal" id="form">
      <div class="form-group">
        <label for="search" class="col-sm-3 control-label">Search query</label>
        <div class="col-sm-9">
          <input type="search" id="search" name="search" class="form-control" data-rule-required="true" /><button type="button" class="form-control-cancel icon-btn icon-cross"></button>
        </div>
      </div>
      <div class="form-group">
        <label for="number" class="col-sm-3 control-label">Number <small>(Lorem ipsum)</small></label>
        <div class="col-sm-3">
          <div class="input-group" data-boxspinner>
            <input type="number"
                   class="form-control"
                   id="number"
                   name="number"
                   min="-2"
                   max="1000.50"
                   step="0.1"
                   data-msg-required="Number is required."
                   data-rule-required="true" />
            <!--<span class="input-group-btn">
              <button class="btn btn-default icon icon-min" type="button" data-stepdown></button>
              <button class="btn btn-default icon icon-plus" type="button" data-stepup></button>
            </span>-->
          </div>
        </div>
      </div>
      <div class="form-group">
        <label for="PromotionImage" class="col-sm-3 control-label required">File</label>
        <div class="col-sm-9">
          <input type="file"
                 id="PromotionImage"
                 name="promotionimage"
                 class="filebtn filebtn-default"
                 data-msg-required="File is required."
                 data-rule-required="true" />
        </div>
      </div>
      <hr />
      <div class="form-group">
        <label for="publishdate" class="col-sm-3 control-label">Publish date</label>
        <div class="col-sm-3">
          <input type="date"
                 id="publishdate"
                 name="publishdate"
                 class="form-control"
                 placeholder="yy-mm-dd"
                 min="2015-03-17"
                 max="2016-11-10"
                 data-msg-required="Publish date is required."
                 data-rule-required="true" />
        </div>
        <label for="publishtime" class="col-sm-3 control-label">Publish time</label>
        <div class="col-sm-3">
          <input type="time"
