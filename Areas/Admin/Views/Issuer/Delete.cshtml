@model nl.boxplosive.BackOffice.Mvc.Areas.Admin.Models.IssuerDeleteModel
@{
    ViewBag.Title = @Model.PageTitle;
}

<h2>@ViewBag.Title</h2>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken();
    @Html.HiddenFor(m => m.Id);
    @Html.HiddenFor(m => m.Version);

    <div class="form-group">
        <p>Do you want to delete account <b>@Model.LoginName</b>?</p>
    </div>
    <div class="form-group">
        <input type="submit" value="Confirm" class="btn btn-default" />
        @Html.ActionLink("Cancel", MVC.AdminArea.Issuer.ActionNames.Index, MVC.AdminArea.Issuer.Name, null, new { @class = "btn btn-link" })
    </div>
}